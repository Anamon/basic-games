10 REM SAILDAT CREATES A DATA FILE CALLED "DATA" FOR SAILOR20 DIM P(29),PR(29),CH(15),SH(15),ST(15),CT(15)30 OPEN "O",#1,"DATA"40 RESTORE50 DATA 0,10,3,1,3,1,3,-3,3,-3,1,-5,1,-5,-1,-5,-1,-5,-3,-3,-3,-3,-3,1,-3,1,0,10,0,560 FOR I=0 TO 2970 READ P(I):P(I)=-P(I):NEXT I80 T=-.3927:X=-9:HW=1.57:Y=1190 V1=36:V2=100:V=4:VW=10:R=9100 FOR I=0 TO 15110 IF I=12 THEN X=-9120 IF I=12 THEN Y=30130 T=T+.3927:X=X+20140 ST(I)=SIN(T):CT(I)=COS(T)150 SH(I)=SIN(3.14-HW+T):CH(I)=COS(T-HW)160 PRINT#1,SH(I),CH(I),ST(I),CT(I)170 V3=V1+V2+2*V*VW*CH(I)180 VR=SQR(V3)190 SW=VW*SH(I)/VR200 C=-(V2-V1-V3)/(2*V*VR)210 CW=ABS(C):W=SGN(SW/CW)220 IF I=10 THEN X=X+1230 FOR J=0 TO 29 STEP 2240 PR(J)=INT(P(J)*CT(I)-P(J+1)*ST(I)+X+.5)250 PR(J+1)=INT(P(J+1)*CT(I)+P(J)*ST(I)+Y+.5)260 PRINT#1,PR(J),PR(J+1)270 NEXT J280 IF T>3.9 AND T<5.5 THEN 300290 GOTO 310300 SW=SGN(W):C=0310 XM=INT(PR(28)-R*C+.5):YM=INT(PR(29)+R*SW+.5)320 PRINT#1,XM,YM330 NEXT I340 CLOSE#1